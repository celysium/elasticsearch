{
    "name": "celysium/elasticsearch",
    "description": "This ORM for DB Elasticsearch",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nasser Hekmati",
            "email": "hekmati.nasser@gmail.com"
        }
    ],
    "require": {
        "elasticsearch/elasticsearch": "^8.0",
        "illuminate/support": ">11.0"
    },
    "autoload": {
        "psr-4": {
            "Celysium\\Elasticsearch\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Celysium\\Elasticsearch\\ElasticsearchServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
